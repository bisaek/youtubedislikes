{

    "manifest_version": 2,
    "name": "youtubedisklike",
    "version": "0.1",
  
    "description": "Adds a red border to all webpages matching mozilla.org.",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.mozilla.org/*"],
        "js": ["index.js"]
      }
    ]
  
  }
  